Cell[TextData[{
 "QRQ 5.1 Using the comments in the cell below as a guide, write a ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " segment to generate a list, ",
 StyleBox["lst",
  FontSlant->"Italic"],
 ", of 30 points.  Initialize the list to be empty and ",
 StyleBox["y",
  FontSlant->"Italic"],
 " to be 1.  Inside the body of a ",
 StyleBox["Do",
  FontSlant->"Italic"],
 " loop with an integer index ",
 StyleBox["i",
  FontSlant->"Italic"],
 " that goes from 0 through 29, make ",
 StyleBox["x",
  FontSlant->"Italic"],
 " be 0.25 times ",
 StyleBox["i",
  FontSlant->"Italic"],
 ", make ",
 StyleBox["y",
  FontSlant->"Italic"],
 " be 1.2 times its previous value, and append the ordered pair of values to \
the list.  After creation of the list of points, plot the points with line \
segments joining adjacent points and labeled axes.  (Notice that the \
resulting graph is similar to the unconstrained-growth, or exponential, graph \
in Figure 3.2.2 of Module 3.2 on \"Unconstrained Growth and Decay.\")"
}], "Subsubsection",
 CellChangeTimes->{{3.455385393352385*^9, 3.455385498634265*^9}, {
   3.4553855477319937`*^9, 3.455385578524293*^9}, 3.757723726244631*^9, {
   3.75776225312173*^9, 
   3.757762257276928*^9}},ExpressionUUID->"54853547-99e5-45ee-85ae-\
d62092da4d83"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
   "initialize", " ", "list", " ", "lst", " ", "to", " ", "be", " ", 
    "empty"}], " ", "*)"}], "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{"initialize", " ", "y", " ", "to", " ", "be", " ", "1"}], " ", 
   "*)"}], "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{
   "for", " ", "i", " ", "going", " ", "from", " ", "0", " ", "through", " ", 
    "29", " ", "in", " ", "a", " ", "loop", " ", "do", " ", "the", " ", 
    RowBox[{"following", ":"}]}], " ", "*)"}], "\[IndentingNewLine]", 
  RowBox[{"(*", "    ", 
   RowBox[{
   "assign", " ", "to", " ", "x", " ", "the", " ", "expression", " ", "0.25", 
    " ", "times", " ", "i"}], " ", "*)"}], "\[IndentingNewLine]", 
  RowBox[{"(*", "    ", 
   RowBox[{
   "assign", " ", "to", " ", "y", " ", "the", " ", "expression", " ", "1.2", 
    " ", "times", " ", "previous", " ", "value", " ", "of", " ", "y"}], " ", 
   "*)"}], "\[IndentingNewLine]", 
  RowBox[{"(*", "    ", 
   RowBox[{
    RowBox[{"assign", " ", "to", " ", "lst", " ", "the", " ", "list"}], ",", 
    " ", 
    RowBox[{
    "with", " ", "ordered", " ", "pair", " ", "of", " ", "x", " ", "and", " ",
      "y", " ", "appended"}]}], " ", "*)"}], "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{"plot", " ", "the", " ", "points"}], " ", "*)"}], 
  "\[IndentingNewLine]", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"Clear", "[", "lst", "]"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"lst", " ", "=", " ", 
     RowBox[{"{", "}"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"y", "=", "1"}], ";"}], "\[IndentingNewLine]", 
   RowBox[{"Do", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"x", "=", " ", 
       RowBox[{"i", "*", ".25"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"y", "=", " ", 
       RowBox[{"1.2", "*", "y"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"lst", " ", "=", " ", 
       RowBox[{"Append", "[", 
        RowBox[{"lst", ",", " ", 
         RowBox[{"{", 
          RowBox[{"x", " ", ",", "y"}], " ", "}"}]}], "]"}]}]}], ",", " ", 
     RowBox[{"{", 
      RowBox[{"i", ",", "0", ",", "29"}], "}"}]}], "\[IndentingNewLine]", 
    "]"}], "\[IndentingNewLine]", 
   RowBox[{"ListLinePlot", "[", 
    RowBox[{"lst", ",", 
     RowBox[{"AxesStyle", "\[Rule]", 
      RowBox[{"{", 
       RowBox[{"\"\<x\>\"", ",", "\"\<y\>\""}], "}"}]}]}], " ", "]"}], 
   "\[IndentingNewLine]"}]}]], "Input",
 CellChangeTimes->{{3.504015014793899*^9, 3.504015160004902*^9}, {
   3.504015221781761*^9, 3.504015255023527*^9}, {3.504015396795713*^9, 
