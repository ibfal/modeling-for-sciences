    RowBox[{"{", 
     RowBox[{
      RowBox[{"-", "3"}], ",", "2"}], "}"}], ",", 
    RowBox[{"{", 
     RowBox[{"2", ",", "2"}], "}"}], ",", 
    RowBox[{"{", 
